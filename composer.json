{
    "name": "digital-threads/liqpay",
    "description": "Laravel LiqPay Client",
    "keywords": [
        "laravel",
        "liqpay",
        "payment"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stas Vartanyan",
            "email": "vaawebdev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DigitalThreads\\LiqPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalThreads\\LiqPay\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.25",
        "liqpay/liqpay": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "fakerphp/faker": "^1.14",
        "squizlabs/php_codesniffer": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.0",
        "vimeo/psalm": "^4.7",
        "orchestra/testbench": "^6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalThreads\\LiqPay\\LiqPayServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs ./src ./config",
        "lint:fix": "phpcbf ./src ./config",
        "psalm": "psalm --show-info=true --no-cache",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
